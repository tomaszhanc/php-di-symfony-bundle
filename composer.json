{
    "name": "tomaszhanc/php-di-symfony-bundle",
    "description": "Provides autowiring controllers for Symfony2 & PHP-DI",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Hanc",
            "email": "tomasz.hanc@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "~2.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "mnapoli/php-di": "5.0.x-dev as 4.99",
        "mnapoli/php-di-symfony2": "@dev"
    },
    "require-dev": {
        "php": ">=5.5",
        "phpunit/phpunit": "4.3.*",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "DI\\Bundle\\Symfony\\": "src/"
        }
    }
}
